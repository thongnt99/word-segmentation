<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<regex_corpus>
	<group type="date">
		<!-- full date -->
		<re desc="mm-dd-yy">(0*[1-9]|1[012])-(0*[1-9]|[12][0-9]|3[01])-\d\d</re>
		<re desc="mm/dd/yy">(0*[1-9]|1[012])/(0*[1-9]|[12][0-9]|3[01])/\d\d</re>
        <re desc="mm.dd.yy">(0*[1-9]|1[012])\.(0*[1-9]|[12][0-9]|3[01])\.\d\d</re>
        <re desc="dd-mm-yy">([12][0-9]|3[01]|0*[1-9])-(1[012]||0*[1-9])-\d\d</re>
        <re desc="dd/mm/yy">([12][0-9]|3[01]|0*[1-9])/(1[012]||0*[1-9])/\d\d</re>
        <re desc="dd.mm.yy">([12][0-9]|3[01]|0*[1-9])[\.](1[012]||0*[1-9])[\.]\d\d</re>
        <re desc="dd-mm-yyyy">([12][0-9]|3[01]|0*[1-9])-(1[012]||0*[1-9])-(19|20)\d\d</re>
        <re desc="dd/mm/yyyy">([12][0-9]|3[01]|0*[1-9])/(1[012]||0*[1-9])/(19|20)\d\d</re>
        <re desc="dd.mm.yyyy">([12][0-9]|3[01]|0*[1-9])\.(1[012]||0*[1-9])\.(19|20)\d\d</re>
        <re desc="mm-dd-yyyy">(0*[1-9]|1[012])-([12][0-9]|3[01]|0*[1-9])-(19|20)\d\d</re>
        <re desc="mm/dd/yyyy">(0*[1-9]|1[012])/([12][0-9]|3[01]|0*[1-9])/(19|20)\d\d</re>
        <re desc="mm.dd.yyyy">(0*[1-9]|1[012])\.([12][0-9]|3[01]|0*[1-9])\.(19|20)\d\d</re>
        <re desc="yyyy-mm-dd">(19|20)\d\d-(0*[1-9]|1[012])-([12][0-9]|3[01]|0*[1-9])</re>
        <re desc="yyyy/mm/dd">(19|20)\d\d/(0*[1-9]|1[012])/([12][0-9]|3[01]|0*[1-9])</re>
        <re desc="yyyy.mm.dd">(19|20)\d\d\.(0*[1-9]|1[012])\.([12][0-9]|3[01]|0*[1-9])</re>
		<!-- date without year -->
        <re desc="dd-mm">(0*[1-9]|[12][0-9]|3[01])[-/\.](1[012]|0*[1-9])</re>
		<!-- month and year -->
        <re desc="mm-yy">(0*[1-9]|1[012])[-/\.]\d\d</re>
        <re desc="mm-yyyy">(0*[1-9]|1[012])[-/\.](19|20)\d\d</re>
		<!-- only year -->
        <re desc="yyyy">(19|20)\d\d</re>
	</group>
	<group type="time">
		<re desc="hh:mm:ss">([0-1]\d|[2][0-3]):[0-5]\d:[0-5]\d</re>
	</group>
	<group type="name">
		<re desc="">([A-ZÁÂĐÐÍÔƯỨÝỶ][aàảãáạăằẳẵắặâầẩẫấậbcdđeèẻẽéẹêềểễếệfghiìỉĩíịjklmnoòỏõóọôồổỗốộơờởỡớợpqrstuùủũúụưừửữứựvwxyỳỷỹýỵz]*)(\s+[A-ZÁÂĐÐÍÔƯỨÝỶ][aàảãáạăằẳẵắặâầẩẫấậbcdđeèẻẽéẹêềểễếệfghiìỉĩíịjklmnoòỏõóọôồổỗốộơờởỡớợpqrstuùủũúụưừửữứựvwxyỳỷỹýỵz]*)*</re>
	</group>
	<group type="address">
		<re desc="email">(\w[-._\w]*\w@\w[-._\w]*\w\.\w{2,3})</re>
	</group>
	<group type="number">
		<re desc="percent">[+-]?(\d*[\.,])?\d+%</re>
		<re desc="degree">[+-]?(\d*[\.,])?\d+°</re>
		<re desc="currency">\p{Sc}\s?[+-]?(\d*[\.,])?\d+|[+-]?(\d*[\.,])?\d+\s?\p{Sc}</re>
	</group>
	<group type="">
	</group>
</regex_corpus>